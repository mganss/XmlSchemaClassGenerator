      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="realtorValuationJSONPLink" >
    <xs:attribute ref="xlink:href">
      <xs:annotation>
        <xs:documentation xml:lang="en">URL for realtor valuation service with jsonp as response (JAVASCRIPT)</xs:documentation>
