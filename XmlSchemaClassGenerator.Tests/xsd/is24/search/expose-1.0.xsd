        <xs:annotation>
          <xs:documentation xml:lang="en">free text message</xs:documentation>
          <xs:documentation xml:lang="de-DE">Freitext Feld</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  
  <xs:complexType name="ImprintLink">
