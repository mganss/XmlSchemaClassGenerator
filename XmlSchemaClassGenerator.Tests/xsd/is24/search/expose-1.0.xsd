  <xs:element name="webContent">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="reportAFraudURL" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">fraudUrlForExpose</xs:documentation>
