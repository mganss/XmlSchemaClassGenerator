				<xsd:attribute name="nameOfPointRefClass" type="NameOfClass" use="optional" fixed="RailwayPoint">
					<xsd:annotation>
						<xsd:documentation>Class of POINT referenced by LINK.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:complexContent>
