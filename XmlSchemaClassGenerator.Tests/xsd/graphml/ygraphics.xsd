                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
          <xs:element name="NodeBounds" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>
