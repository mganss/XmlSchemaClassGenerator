            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="autoSizePolicy" type="autoSizePolicy.type" use="optional">
          <xs:annotation>
            <xs:documentation>
              The auto size policy of the label.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="borderDistance" type="xs:double" use="optional">
          <xs:annotation>
            <xs:documentation>
