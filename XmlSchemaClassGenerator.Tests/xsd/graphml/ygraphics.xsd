	<xs:complexType>
	  <xs:complexContent>
        <xs:extension base="TableNodeLabelModelParameter.type">
		  <xs:attribute name="verticalPosition" type="xs:double" use="optional">
            <xs:annotation>
              <xs:documentation>
                The vertical position of the label.
