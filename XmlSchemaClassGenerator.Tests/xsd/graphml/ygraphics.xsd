          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ratio" type="xs:double" use="optional">
          <xs:annotation>
            <xs:documentation>
              The current label position along the edge, expressed as ratio from source to target.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="TableNodeLabelModel.type">
