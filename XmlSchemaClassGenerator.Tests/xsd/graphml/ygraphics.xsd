        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="right" type="xs:double" use="optional">
      <xs:annotation>
        <xs:documentation>
          The minimal inset at the right in double precision.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="Insets.type">
    <xs:annotation>
      <xs:documentation>
