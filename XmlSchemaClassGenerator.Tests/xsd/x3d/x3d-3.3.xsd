			<xs:appinfo>
				<xs:attribute name="isActive" type="SFBool" fixed="outputOnlyField"/>
				<xs:attribute name="componentName" type="componentNameChoices" fixed="Followers"/>
				<xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="1"/>
