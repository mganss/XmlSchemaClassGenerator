						</xs:element>
					</xs:sequence>
					<xs:attribute name="bboxCenter" type="SFVec3f" default="0 0 0"/>
					<xs:attribute name="bboxSize" type="bboxSizeType" default="-1 -1 -1"/>
					<xs:attribute name="center" type="SFVec3f" default="0 0 0"/>
