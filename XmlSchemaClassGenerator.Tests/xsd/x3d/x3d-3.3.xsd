					<xs:attribute name="skinCoordWeight" type="MFFloat"/>
                    <xs:attribute name="llimit" type="MFFloat" default="0 0 0">
						<xs:annotation>
                            <xs:appinfo>This field shall contain three values or else be an empty array. X3D Working group declined to fix type as SFVec3f in X3D version 4.0, maintaining exact backwards compatibility. Mantis 1116 https://www.web3d.org/member-only/mantis/view.php?id=1116</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
                    <xs:attribute name="ulimit" type="MFFloat" default="0 0 0">
