						<xs:element ref="NurbsSweptSurface"/>
						<xs:element ref="NurbsSwungSurface"/>
						<xs:element ref="NurbsTrimmedSurface"/>
						<xs:element ref="ProtoInstance">
							<xs:annotation>
								<xs:documentation>Appropriately typed substitution node</xs:documentation>
							</xs:annotation>
