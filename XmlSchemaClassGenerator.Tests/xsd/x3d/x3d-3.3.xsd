	<!-- Public and private schema extension mechanisms -->
	<xs:include schemaLocation="x3d-3.3-Web3dExtensionsPublic.xsd"/>
	<xs:include schemaLocation="x3d-3.3-Web3dExtensionsPrivate.xsd"/>
