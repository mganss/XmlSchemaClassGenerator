				<xs:sequence>
					<xs:group ref="SceneGraphFragmentWithPrototypeDeclarationsContentModel" maxOccurs="unbounded"/>
				</xs:sequence>
							<xs:attribute name="timestamp" use="required">
                                                            < ! - - type="SFTimeNonNegative" - - >
                                                            <xs:simpleType>
                                                                    <xs:restriction base="SFTime">
                                                                            <xs:minInclusive value="0"/>
