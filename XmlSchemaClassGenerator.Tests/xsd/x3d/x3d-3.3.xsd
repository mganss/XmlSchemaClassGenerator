					<xs:attribute name="maxTorque1" type="SFFloat" default="0"/>
					<xs:attribute name="maxTorque2" type="SFFloat" default="0"/>
					<xs:attribute name="minAngle1" type="SFFloat" default="-3.141592653"/>
